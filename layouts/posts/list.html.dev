{{ define "main" }}

    <h1>{{ i18n .Title }}</h1>
    
    {{ range (.Site.RegularPages.GroupByDate "2006") }}
    <h3>{{ .Key }}</h3>
	
    <ul>
    {{ range (where .Pages "Type" "posts") }}
    	<li>
    		<a href="{{ .RelPermalink }}">{{ .Title }}</a>  
    	
    	</li>
    {{ end }}
    </ul>
    
    {{ end }}
{{ end }}
